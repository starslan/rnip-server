<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:com="http://rnip.mos.ru/xsd/Common/2.6.0"
			xmlns:pkg="http://rnip.mos.ru/xsd/Package/2.6.0"
			targetNamespace="urn://rnip.mos.ru/xsd/services/import-payments/2.6.0"
			elementFormDefault="qualified">

	<xsd:import namespace="http://rnip.mos.ru/xsd/Common/2.6.0" schemaLocation="commons/Common.xsd"/>
	<xsd:import namespace="http://rnip.mos.ru/xsd/Package/2.6.0" schemaLocation="commons/Package.xsd"/>

	<xsd:element name="ImportPaymentsRequest">
		<xsd:annotation>
			<xsd:documentation>Прием информации об уплате (информация из распоряжения плательщика)</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="com:RequestType">
					<xsd:sequence>
						<xsd:element ref="pkg:PaymentsPackage"/>
					</xsd:sequence>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="ImportPaymentsResponse" type="com:ImportPackageResponseType">
		<xsd:annotation>
			<xsd:documentation>Ответ на запрос приема информации об уплате (информации из распоряжения плательщика)
			</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
</xsd:schema>
