												</xs:annotation>
											</xs:element>
										</xs:sequence>
									</xs:restriction>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="PaymentsConditions" type="sc:PaymentsConditionsType">
							<xs:annotation>
								<xs:documentation>Условия для получения информации по УПНО</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="TimeConditions" type="sc:TimeConditionsType">
